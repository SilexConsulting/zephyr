# Heltec Wireless Stick Lite (V3) board configuration
# SPDX-License-Identifier: Apache-2.0

config BOARD_HELTEC_WIRELESS_TRACKER
	select SOC_ESP32S3_FN8
	select SOC_ESP32S3_PROCPU if BOARD_HELTEC_WIRELESS_TRACKER_ESP32S3_PROCPU
	select SOC_ESP32S3_APPCPU if BOARD_HELTEC_WIRELESS_TRACKER_ESP32S3_APPCPU
